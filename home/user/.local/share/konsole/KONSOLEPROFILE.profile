[Appearance]
ColorScheme=Linux
Font=Fira Code,12,-1,5,75,0,0,0,0,0,Bold
LineSpacing=1
TabColor=128,255,255
UseFontLineChararacters=false

[Cursor Options]
CursorShape=1

[General]
AlternatingBackground=2
Command=/bin/zsh
Environment=TERM=xterm-256color,COLORTERM=truecolor
ErrorBackground=2
Name=KONSOLEPROFILE
