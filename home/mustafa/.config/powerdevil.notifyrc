[Event/criticalbattery]
Execute=
Logfile=
Sound=/usr/share/sounds/freedesktop/stereo/alarm-clock-elapsed.oga
TTS=

[Event/fullbattery]
Action=Sound|Popup
Execute=
Logfile=
Sound=/usr/share/sounds/Oxygen-K3B-Insert-Medium.ogg
TTS=

[Event/lowbattery]
Execute=
Logfile=
Sound=/usr/share/sounds/Oxygen-Window-Shade-Up.ogg
TTS=

[Event/lowperipheralbattery]
Execute=
Logfile=
Sound=/usr/share/sounds/kapman/gameover.ogg
TTS=

[Event/pluggedin]
Action=Sound|Popup
Execute=
Logfile=
Sound=/usr/share/sounds/Oxygen-Im-New-Mail.ogg
TTS=%e

[Event/unplugged]
Action=Sound|Popup
Execute=
Logfile=
Sound=/usr/share/sounds/Oxygen-Window-All-Desktops.ogg
TTS=
